#include<stdio.h>
#include<string.h>
#include<stdlib.h>
main()
{
    int i,c,l,j;
    char s[1000],a[11]="0123456789";
    gets(s);
    l=strlen(s);
    for(i=0;i<10;i++){
        c=0;
        for(j=0;j<l;j++){
            if(s[j]==a[i]){
                c++;
            }
        }
        printf("%d ",c);
    }
}
